			<dict>
				<key>moduleType</key>
				<string>ASDisplayNode</string>
			</dict>
		</dict>
		<dict>
			<key>Default</key>
			<string>___VARIABLE_textureKitModuleName___</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>Type</key>
			<string>static</string>
