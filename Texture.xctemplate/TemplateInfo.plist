			<key>Default</key>
			<string>ASDisplayNode</string>
			<key>Required</key>
			<true/>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>_Base</string>
			<key>Name</key>
			<string>Use BaseNode</string>
