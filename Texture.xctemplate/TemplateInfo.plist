			<string>text</string>
			<key>Placeholder</key>
			<string>Input Module Name</string>
			<key>Required</key>
			<true/>
			<key>NotPersisted</key>
			<true/>
		</dict>
		<dict>
			<key>Description</key>
			<string>Module type</string>
			<key>Identifier</key>
			<string>moduleType</string>
